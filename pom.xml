<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <artifactId>amt-tiltak</artifactId>
    <groupId>no.nav.amt-tiltak</groupId>
    <version>1.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <modules>
        <module>application</module>
        <module>core</module>
        <module>connectors</module>
        <module>tiltaksleverandor</module>
        <module>tiltak</module>
        <module>ingestors</module>
        <module>db-migrations</module>
        <module>tools</module>
        <module>mocks</module>
    </modules>

    <name>AMT-Tiltak</name>

    <parent>
        <artifactId>spring-boot-starter-parent</artifactId>
        <groupId>org.springframework.boot</groupId>
        <version>2.5.2</version>
    </parent>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <kotlin.code.style>official</kotlin.code.style>
        <kotlin.compiler.jvmTarget>1.8</kotlin.compiler.jvmTarget>
        <maven.compiler.source>15</maven.compiler.source>
        <maven.compiler.target>15</maven.compiler.target>

        <testcontainers.version>1.16.0</testcontainers.version>
        <mockito-kotlin.version>3.2.0</mockito-kotlin.version>
        <schedlock.version>4.27.0</schedlock.version>

        <common.version>2.2021.08.16_13.46-d90f8c15f6ff</common.version>
        <wiremock.version>2.31.0</wiremock.version>
        <nimbus.version>9.18</nimbus.version>
        <mockk.version>1.12.0</mockk.version>
        <kotest.version>5.0.0.M2</kotest.version>
        <okhttp.version>4.9.2</okhttp.version>
        <token-support.version>1.3.8</token-support.version>
        <mock-oauth2-server.version>0.3.5</mock-oauth2-server.version>
        <common.version>2.2021.08.16_13.46-d90f8c15f6ff</common.version>
    </properties>

    <repositories>
        <repository>
            <id>mavenCentral</id>
            <url>https://repo1.maven.org/maven2/</url>
        </repository>
    </repositories>

    <build>
        <sourceDirectory>src/main/kotlin</sourceDirectory>
        <testSourceDirectory>src/test/kotlin</testSourceDirectory>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>3.0.0-M4</version>
            </plugin>
            <plugin>
                <groupId>org.jetbrains.kotlin</groupId>
                <artifactId>kotlin-maven-plugin</artifactId>
                <version>1.5.10</version>
                <executions>
                    <execution>
                        <id>compile</id>
                        <phase>compile</phase>
                        <goals>
                            <goal>compile</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>test-compile</id>
                        <phase>test-compile</phase>
                        <goals>
                            <goal>test-compile</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

    <dependencyManagement>
        <dependencies>
